    <div class="table-responsive">
        <table class="table table-hover">
            <thead>
                <tr class="active">
                    <th>ニックネーム</th>
                    <th>ログイン状態</th>
                    <th></th>
                </tr>
            </thead>
            <tbody id="accountTable" data-bind="foreach: accountList">
                <tr>
                    <td data-bind="text: name"></td>
                    <td><span class="label label-default">未ログイン</span></td>
                    <td><button class="btn btn-default" data-toggle="modal" data-target="#messageForm">メッセージ</button></td>
                </tr>
            </tbody>
        </table>
    </div>
    
    <form class="form-horizontal">
        <div class="modal fade" id="messageForm" tabindex="-1" role="dialog" aria-labelledby="messageFormLabel" aria-hidden="true">
            <div class="modal-dialog">
                <div class="modal-content">
                    <div class="modal-header">
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                        <h4 class="modal-title" id="messageFormLabel">メッセージ送信</h4>
                    </div>
                    <div class="modal-body"><textarea class="form-control"></textarea>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-default" data-dismiss="modal">閉じる</button>
                        <button data-bind='click: addGift' class="btn btn-primary" >送信する</button>
                    </div>
                </div>
            </div>
        </div>
    </form>
    <script type="text/javascript" src="/viewModel/viewAccountModel.js"></script>